{
    "JwtSettings": {
        "SecretKey": "my top secret key",
        "Issuer": "myissuer",
        "Audience": "myaudience",
        "ExpirationSeconds": 10000

    },
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "AllowedHosts": "*",

    "ConnectionStrings": {
        "MyDatabase": "Server=(localdb);Database=online store;Trusted_Connection=True;"
    },

    "Serilog": {
        "MinimumLevel": "Information",
        "Using": [
            "Serilog.Sinks.Console"
        ],
        "WriteTo": [
            {
                "Name": "Console"
            }
        ]
    },
    "Redis" :  "localhost:6379"


}
